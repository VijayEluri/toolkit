<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE project PUBLIC "-//ANT//DTD project//EN"
  "file:///home/oliver/devel/doc/project.dtd">
<!--<!DOCTYPE project>-->

<project name="trailmagic-toolkit" default="build" basedir=".">
  <!-- REMEMBER TO EDIT THIS AND ABOVE -->
  <property name="project.name" value="trailmagic-toolkit"/>
  
  <!-- potentially override what's in here -->
  <property file="ant.properties"/>


    

  <target name="clean">
    <ant dir="user" inheritAll="false" target="clean"/>
    <ant dir="imagestore" inheritAll="false" target="clean"/>
  </target>

  <target name="distclean" depends="clean">
    <ant dir="user" inheritAll="false" target="distclean"/>
  </target>
  
  <target name="init">
    <ant dir="user" inheritAll="false" target="init"/>
  </target>

  <target name="build">
    <ant dir="user" inheritAll="false" target="build"/>
  </target>

  <target name="deploy">
    <ant dir="user" inheritAll="false" target="deploy"/>
    <ant dir="imagestore" inheritAll="false" target="deploy"/>
  </target>

  <target name="compile">
    <ant dir="user" inheritAll="false" target="compile"/>
  </target>

  
</project>
